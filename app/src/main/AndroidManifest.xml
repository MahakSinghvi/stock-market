<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ps.sm">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_app"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/icon_app"
        android:supportsRtl="true"
        android:theme="@style/Theme.StockMarketApp">
        <activity android:name=".activity.ResultActivity"></activity>
        <activity android:name=".activity.StrategyActivity" />
        <activity android:name=".activity.FillValuesActivity" />
        <activity android:name=".activity.AddActivity" />
        <activity android:name=".activity.ItemFilterActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>